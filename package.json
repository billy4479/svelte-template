    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "snowpack": "^3.0.1",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.6",
    "ts-jest": "^26.5.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
